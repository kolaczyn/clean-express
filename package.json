    "eslint": "^8.23.0",
    "eslint-plugin-jest": "^26.8.7",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
